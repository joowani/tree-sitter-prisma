{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "prisma",
      "camelcase": "Prisma",
      "title": "Prisma",
      "scope": "source.prisma",
      "file-types": [
        "prisma"
      ],
      "injection-regex": "^prisma$",
      "class-name": "TreeSitterPrisma"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Prisma grammar for tree-sitter",
    "authors": [
      {
        "name": "Joohwan Oh",
        "email": "joohwan.oh@outlook.com",
        "url": "https://github.com/joowani"
      }
    ],
    "links": {
      "repository": "https://github.com/joowani/tree-sitter-prisma"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}